{
  "$schema": "https://railway.app/railway.schema.json",
  "build": {
    "builder": "DOCKERFILE",
    "dockerfilePath": "Dockerfile"
  },
  "deploy": {
    "startCommand": "dumb-init sh -c 'yarn db:migrate && node build/server/index.js --services=cron,collaboration,websockets,admin,web,worker'",
    "restartPolicyType": "ON_FAILURE",
    "restartPolicyMaxRetries": 10
  },
  "volumes": [
    {
      "name": "outline-data",
      "mountPath": "/var/lib/outline/data"
    }
  ]
}